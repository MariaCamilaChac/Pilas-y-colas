
public class pilas {
	import java.io.BufferedWriter;
	import java.io.IOException;
	import java.io.OutputStreamWriter;

	public class List {
		
		Node head=null; //Empty list
		
		public boolean isEmpty() {
			return head == null? true: false;
		}
		
		public void Push(Node newNode) {
			newNode.next = head;
			head = newNode;
			
		}
		
		public void Queque(Node newNode) {
			
			Node temp = head;
			while (temp.next != null)
				temp = temp.next;
			
			temp.next = newNode;
			
		}
		
		public Node Pop() {
			Node temp=head;
			head=head.next;
			return temp;
			
		}
		public void Deque() {
			Node pre = head;
			Node temp;
			while (pre.next.next != null)
				pre = pre.next;
			
			temp = pre.next;
			pre.next=null;
			System.gc();
			
		}
		
		public void PrintList() throws IOException {
			BufferedWriter bw = new BufferedWriter (new OutputStreamWriter(System.out));
			Node temp = head;
			
			while (temp != null) {
				bw.write (temp.toString()+"\n");
				temp=temp.next;
				
			}
			bw.flush();
			bw.close();
		}
		
		
		public static void main(String[] args) throws IOException {
			
			List student = new List();
			student.Push(new Node ("Paula", 22, 561556));
			student.Queque(new Node("camila ", 22,2434));
			student.Push(new Node("Juan", 23,2345676));
			student.Pop();
			System.out.println(student.Pop());
			student.PrintList();

			
		}

	}


}
